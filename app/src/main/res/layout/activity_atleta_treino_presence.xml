<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AtletaTreinoPresence">

    <ImageButton
            android:layout_width="90dp"
            android:background="@drawable/buttonshape"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_menu_revert"
            android:id="@+id/back"
            tools:layout_editor_absoluteX="16dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="600dp"
    />
    <TextView
            android:text="PRESENÇAS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="139dp" android:id="@+id/textView9" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="80dp"
            android:textColor="@color/colorPrimaryDark" android:textStyle="bold"/>

    <android.support.design.widget.TabLayout
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            app:tabMode="scrollable"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:tabIndicatorColor="@color/colorPrimary"
            app:tabIndicatorGravity="stretch" app:layout_constraintTop_toBottomOf="@+id/tabLayout2"
            android:layout_marginTop="1dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp">

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Semana1"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Semana2"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Semana3"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Semana4"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Semana5"/>

    </android.support.design.widget.TabLayout>

    <android.support.design.widget.TabLayout
            android:layout_width="393dp"
            android:layout_height="wrap_content"
            app:tabMode="scrollable"
            app:tabIndicatorColor="@color/colorPrimary"
            app:tabIndicatorGravity="stretch" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/tabLayout" android:id="@+id/tabLayout2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp">

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Janeiro"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fevereiro"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Março"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Abril"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Maio"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Junho"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Setembro"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Outubro"/>

        <android.support.design.widget.TabItem
                android:layout_width="wrap_content"
